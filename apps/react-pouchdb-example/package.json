{
  "name": "example-app",
  "version": "0.1.0",
  "description": "A react app using pouchdb built with parcel in typescript",
  "author": "Stan Lemon <stanlemon@users.noreply.github.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:stanlemon/parcel-app.git"
  },
  "main": "./src/index.tsx",
  "scripts": {
    "start": "webpack serve",
    "test": "jest",
    "build": "tsc --build tsconfig.json ./",
    "prettier": "prettier --write \"src/**/*\"",
    "lint": "eslint --ext js,jsx,ts,tsx ./src/"
  },
  "browserslist": [
    "last 1 year, not dead",
    "edge > 18",
    "not ie 11",
    "not ie_mob 11",
    "not samsung < 1000",
    "not opera < 1000",
    "not kaios < 1000",
    "not android < 1000",
    "not and_qq < 1000"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "roots": [
      "./src/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "^typeface-(.*)": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@stanlemon/react-couchdb-authentication": "^0.6.3",
    "@stanlemon/react-pouchdb": "^0.2.9",
    "bulma": "^0.9.3",
    "date-fns": "^2.28.0",
    "pouchdb": "^7.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@hot-loader/react-dom": "^17.0.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/pouchdb": "^6.4.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/uuid": "^8.3.4",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "pouchdb-adapter-memory": "^7.2.2",
    "prettier": "^2.5.1",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
