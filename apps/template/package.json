{
  "name": "@stanlemon/app-template",
  "version": "0.3.28",
  "description": "A template for creating apps using the webdev package.",
  "author": "Stan Lemon <stanlemon@users.noreply.github.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "start:prod": "NODE_ENV=production node app.js",
    "start:dev": "NODE_ENV=development nodemon --ignore ./db.json app.js",
    "build": "npm run webpack:build",
    "tsc": "tsc",
    "webpack:serve": "webpack serve",
    "webpack:build": "NODE_ENV=production webpack",
    "dev": "NODE_ENV=development concurrently \"npm run webpack:serve\" \"npm run start:dev\"",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest -w",
    "test:coverage": "jest --coverage",
    "lint": "eslint --ext js,jsx,ts,tsx ./src/",
    "lint:fix": "eslint --fix --ext js,jsx,ts,tsx ./src/"
  },
  "dependencies": {
    "@stanlemon/server-with-auth": "*",
    "@stanlemon/webdev": "*",
    "react": "^18.2.0",
    "react-cookie": "^7.0.2",
    "react-dom": "^18.2.0",
    "wouter": "^2.12.1"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "concurrently": "^8.2.2",
    "supertest": "^6.3.4"
  }
}