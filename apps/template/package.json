{
  "name": "@stanlemon/app-template",
  "version": "0.3.65",
  "description": "A template for creating apps using the webdev package.",
  "author": "Stan Lemon <stanlemon@users.noreply.github.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "start:prod": "NODE_ENV=production node app.js",
    "start:dev": "NODE_ENV=development nodemon --ignore ./db.json app.js",
    "build": "npm run webpack:build",
    "tsc": "tsc",
    "webpack:serve": "NODE_ENV=development webpack serve",
    "webpack:build": "NODE_ENV=production webpack",
    "dev": "NODE_ENV=development concurrently \"npm run webpack:serve\" \"npm run start:dev\"",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest -w",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "engines": {
    "node": "^20.0.0"
  },
  "dependencies": {
    "@stanlemon/server-with-auth": "*",
    "@stanlemon/webdev": "*",
    "react": "^19.0.0",
    "react-cookie": "^7.2.2",
    "react-dom": "^19.0.0",
    "wouter": "^3.3.5"
  },
  "devDependencies": {
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0"
  }
}