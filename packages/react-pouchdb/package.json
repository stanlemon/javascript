{
  "name": "@stanlemon/react-pouchdb",
  "version": "0.6.11",
  "description": "React components for interacting with PouchDB.",
  "keywords": [
    "react",
    "pouchdb",
    "couchdb"
  ],
  "author": "Stan Lemon <stanlemon@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stanlemon/javascript/tree/main/packages/react-pouchdb"
  },
  "type": "module",
  "exports": [
    "./src/index.ts"
  ],
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest -w",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "clean": "rimraf ./dist/",
    "build": "rimraf ./dist/; tsc; NODE_ENV=production babel --config-file ./.babelrc.json --extensions=\".ts,.tsx,.js.,.jsx\" ./src/ --out-dir ./dist/ --ignore  \"src /**/*.test.*\" --source-maps",
    "tsc": "tsc",
    "example": "WEBDEV_ENTRY=./example/Example.tsx webpack serve"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "peerDependencies": {
    "pouchdb": ">=8.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@stanlemon/webdev": "*",
    "@types/debug": "^4.1.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.9.0",
    "@types/pouchdb": "^6.4.2",
    "pouchdb": "^9.0.0",
    "pouchdb-adapter-memory": "^9.0.0",
    "rimraf": "^6.0.1",
    "wait-for-expect": "^3.0.2"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "overrides": {
    "@types/pouchdb-core": "npm:@stanlemon/types-pouchdb-core@^0.0.1"
  }
}